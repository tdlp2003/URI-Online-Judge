import java.util.Scanner;

public class Main
{
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        int x;
        double y, consumo;

        x = sc.nextInt();
        y = sc.nextDouble();

        consumo = x / y;

        System.out.println(String.format("%.3f km/l", consumo));
    }
}
