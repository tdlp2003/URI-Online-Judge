import java.util.Scanner;

public class Main
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);

        int [] a = new int[100];
        int max = 0, pmax = 0;

        for (int i = 0; i < a.length;++i )
        {
            a[i] = sc.nextInt();

            if (a[i] > max)
            {
                max = a[i];
                pmax = i+1;

            }


        }
        System.out.println(max);
        System.out.println(pmax);

    }
}
