import java.util.Scanner;

public class Main
{
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        double n, raio, area;

        n = 3.14159;
        raio = sc.nextDouble();

        area = raio * raio * n;

        System.out.println(String.format("A=%.4f", area));


    }
}
