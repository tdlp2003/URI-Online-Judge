import java.util.Scanner;

public class Main{

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        double a;
        int i;
        double b[] = new double[100];
        for (i = 0; i <= 99; i++){
            a = sc.nextDouble();
            if (a <=10) {
                System.out.println("A[" + i + "] = " + a);

            a++;
        }
      }
   }
}
